{
  "version": 2,
  "builds": [
    { "src": "api/search.js", "use": "@vercel/node" },
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "manifest.json", "use": "@vercel/static" },
    { "src": "icons/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/search", "dest": "/api/search.js" },
    { "src": "/manifest.json", "dest": "/manifest.json" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
